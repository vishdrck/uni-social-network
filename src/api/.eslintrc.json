{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": ["prettier"],

    "plugins": ["prettier"],

    "rules": {
          "prettier/prettier": ["error"]
    },
      
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    }
}